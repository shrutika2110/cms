{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "shortDes": {
      "type": "text",
      "required": true
    },
    "coverImg": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slider": {
      "type": "component",
      "repeatable": true,
      "component": "slider.slider"
    },
    "publish_date": {
      "type": "date"
    },
    "firstFold": {
      "type": "component",
      "repeatable": false,
      "component": "fold.fold"
    },
    "secondFold": {
      "type": "component",
      "repeatable": false,
      "component": "fold.fold"
    },
    "thirdFold": {
      "type": "component",
      "repeatable": false,
      "component": "fold.fold"
    },
    "fourthFold": {
      "type": "component",
      "repeatable": false,
      "component": "fold.fold"
    },
    "fifthFold": {
      "type": "component",
      "repeatable": false,
      "component": "fold.fold"
    },
    "sixthFold": {
      "type": "component",
      "repeatable": false,
      "component": "fold.fold"
    },
    "good_read": {
      "type": "boolean"
    },
    "hero_slider": {
      "type": "boolean"
    },
    "recommended": {
      "type": "boolean"
    },
    "pick": {
      "type": "boolean"
    },
    "watch": {
      "type": "boolean"
    },
    "video": {
      "type": "component",
      "repeatable": false,
      "component": "video.video"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "blog_authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-author.blog-author",
      "inversedBy": "blogs"
    },
    "blog_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-category.blog-category",
      "inversedBy": "blogs"
    },
    "views": {
      "type": "biginteger"
    },
    "seventhFold": {
      "type": "component",
      "repeatable": false,
      "component": "fold.fold"
    },
    "sub_description": {
      "type": "string"
    },
    "videoViews": {
      "type": "biginteger"
    },
    "trending": {
      "type": "boolean"
    },
    "readDuration": {
      "type": "string"
    },
    "recommendeds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::recommended.recommended",
      "inversedBy": "blogs"
    }
  }
}
